{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-console": 0,
    "require-jsdoc": "off",
    "no-tabs": 0,
    "no-trailing-spaces": "off",
    "indent": "off",
    "padded-blocks": "off",
    "prefer-default-export": "off",
    "no-prototype-builtins": "off",
    "no-mixed-spaces-and-tabs": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./src"]]
      },
      "extensions": [".js"]
    }
  }
}
